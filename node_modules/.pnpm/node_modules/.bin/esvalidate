#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rajdeepsingh/documention/awesome-nextjs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/rajdeepsingh/documention/awesome-nextjs/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esprima/bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../esprima/bin/esvalidate.js" "$@"
fi
